{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.js"
      }
    ],
    "projectSettings": {
        "framework": "nextjs",
        "buildCommand": "npm run build",
        "outputDirectory": "out",
        "publicSource": true,
        "environmentVariables": {
          "NODE_ENV": "production"
        }
      }
  }
  